{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "no-undef": 0,
        "max-len": 0,
        "no-restricted-syntax": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-console": 0
    }
}
